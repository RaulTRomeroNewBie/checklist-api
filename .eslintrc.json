{
    "env": {
        "node": true,
        "es2021": true,
        "module": true,
        "commonjs": false
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": [
            "error",
            {
                "argsIgnoredPattern": "next"
            }
        ]
    }
}
